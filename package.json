{
  "name": "backend-nodejs",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn service:main",
    "service:main": "node ./dist/runners/main",
    "service:api": "node ./dist/runners/api",
    "service:api:dev": "npx nodemon ./runners/api.ts",
    "env:config": "ts-node ./config/secretManager",
    "build": "rimraf ./dist && tsc",
    "db:create": "ts-node ./src/models/utils/create.ts",
    "db:migrate": "ts-node ./src/models/utils/migrate.ts",
    "db:migrate:create": "sequelize migration:create --migrations-path src/models/pg/migrations/  --name"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.299.0",
    "@aws-sdk/client-secrets-manager": "^3.299.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@sentry/node": "^7.45.0",
    "@types/lodash": "^4.14.191",
    "amazon-cognito-identity-js": "^6.2.0",
    "async": "^3.2.4",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "crypto-js": "^4.1.1",
    "decimal.js": "^10.4.3",
    "disposable-email-domains": "^1.0.62",
    "dotenv": "^16.0.3",
    "elastic-apm-node": "^3.43.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.4.0",
    "koa-compose": "^4.1.0",
    "koa-jwt": "^4.0.4",
    "lodash": "^4.17.21",
    "logzio-nodejs": "^2.1.5",
    "moment": "^2.29.4",
    "node-rsa": "^1.1.1",
    "pg": "^8.10.0",
    "sequelize": "^6.30.0",
    "speakeasy": "^2.0.0",
    "toobusy-js": "^0.5.1",
    "ua-parser-js": "^1.0.34",
    "umzug": "^3.2.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.18",
    "@types/bcrypt": "^5.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/koa": "^2.13.5",
